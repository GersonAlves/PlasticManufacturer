<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <title>F&amp;D PLASTICS | Exellence In Color</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/css/images/favicon.ico" />
    <link rel="stylesheet" href="~/css/style.css" type="text/css" media="all" />
    <link rel="stylesheet" href="~/css/facebox.css" type="text/css" media="all" />
    <!--[if IE 6]><link rel="stylesheet" href="css/ie6.css" type="text/css" media="all" /><![endif]-->
    
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>*@
    
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    



</head>
<body>
    <div id="wrapper">
        <!-- Header -->
        <div id="header">
            <!-- Shell -->
            <div class="shell">
                <a href="#" class="menu-opener"><span></span></a>
                <!-- Logo -->
                <h1 id="logo" class="fl notext"><a href="#">F&amp;D PLASTICS | Exellence In Color</a></h1>
                <!-- END Logo -->
                <!-- Header Right -->
                <div class="header-right fr">
                    <ul class="fr">
                        <li class="last">
                            <!-- Utility -->
                            <div class="utility">
                                <div class="blue-box">
                                    <div class="blue-box-r">
                                        <p><strong>Olá Aulus</strong><span>|</span><a href="#" class="logout">Sair »</a></p>
                                    </div>
                                </div>
                            </div>
                            <!-- END Utility -->
                        </li>
                    </ul>
                </div>
                <!-- END Header Right -->
            </div>
            <!-- END Shell -->
        </div>
        <!-- END Header -->
        <div id="content">
            <!-- Shell -->
            <div class="shell">
                <!-- Navigation -->
                <div id="navigation">
                   <ul class="nav nav-tabs">
                        <li><a data-toggle="tab" href="#table">Table</a></li>
                        <li><a data-toggle="tab" href="#maintain">Maintain</a></li>
                        <li><a data-toggle="tab" href="#tasks">Tasks</a></li>
                        <li><a data-toggle="tab" href="#reports">Reports</a></li>
                        @*<li><a href="#">Administration</a></li>
                        <li><a href="#">Registration</a></li>
                        <li><a href="#">Formulas</a></li>
                        <li class="last"><a href="#">FAQ</a></li>*@
                    </ul>
                </div>
                <!-- END Navigation -->
                <!-- Sub Navigation -->
                <div class="tab-content">
                    <div id="table" class="tab-pane fade">
                        <div class="sub-nav">
                            <ul>
                                <li>@Html.ActionLink("Category", "index", "Categories")</li>
                            </ul>
                        </div>
                    </div>
                    <div id="maintain" class="tab-pane fade">
                        <div class="sub-nav">
                            <ul>
                                <li>@Html.ActionLink("Customer", "index", "Customers")</li>
                                <li><a href="#">Defaults</a></li>
                                <li><a href="#">Employee</a></li>
                                <li><a href="#">Supplier</a></li>
                                <li>@Html.ActionLink("Raw Material", "index", "RawMaterials")</li>
                            </ul>
                        </div>
                    </div>
                    <div id="tasks" class="tab-pane fade">
                        <div class="sub-nav">
                            <ul>
                                <li><a href="#">Color Match Request</a></li>
                            </ul>
                        </div>
                    </div>
                    <div id="reports" class="tab-pane fade">
                        <div class="sub-nav">
                            <ul>
                                <li><a href="#"></a></li>
                                @*<li><a href="#">Production</a></li>
                                <li><a href="#">Priorities</a></li>
                                <li><a href="#">Administration Pages</a></li>
                                <li><a href="#">Client Central</a></li>
                                <li><a href="#">Color Match Request</a></li>
                                <li class="last"><a href="#">Customer Services</a></li>*@
                            </ul>
                        </div>
                    </div>
                </div>
                
                <!-- END Sub Navigation --> 
            </div>
            <!-- END Shell -->
        </div>

        <!--Content-->
        <div id="content">
            <!-- Shell -->
            <div class="shell">
                <!-- White Box -->
                <div class="white-box">
                    <div class="cnt">
                    @RenderBody()
                    </div>
                </div>
                <!-- END White Box -->
            </div>
            <!-- END Shell -->
        </div>
        <!-- END Content -->

        <!-- Footer -->
        <div id="footer">
            <!-- Shell -->
            <div class="shell">
                <p class="ac">&copy; Copyright 2009-2010 - All rights reserved. F&amp;D Plastics Administrative Panel - Design by <a href="#">Dellustrations.com</a></p>
            </div>
            <!-- END Shell -->
        </div>
        <!-- END Footer -->

    </div>

</body>
</html>
